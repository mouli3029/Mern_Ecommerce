 false\r\n    })\r\n\r\n    const {email,password,loading,error,didRedirect} = values;\r\n    const {user} = isAuthenticated();\r\n\r\n    const handleChange = name => event =>{\r\n        /*         [name] changes according to the value we passs through the handleChange\r\n         */        setValues({...values,error:false, [name]:event.target.value});\r\n                   \r\n    } \r\n\r\n    const onSubmit  = event =>{\r\n        event.preventDefault();\r\n        setValues({...values,error : false,loading:true})\r\n        signin({\r\n            email,password\r\n        })\r\n        .then(data =>{\r\n            if(data.error){\r\n                setValues({...values,error : data.error,loading:false})\r\n            }\r\n            else{\r\n                authenticate(data,()=>{\r\n                    setValues({\r\n                    ...values,\r\n                    email : \"\",\r\n                    password : \"\",\r\n                    error : \"\",\r\n                    loading: false,\r\n                    didRedirect: true,\r\n\r\n                })\r\n                })\r\n            }\r\n        })\r\n        .catch(console.log(\"Signin request Failed\"))\r\n    }\r\n\r\n    const performRedirect = ()=>{\r\n        if(didRedirect){\r\n            if(user && user.role === 1){\r\n                return <Redirect to='/admin/dashboard'/>\r\n            }\r\n            else{\r\n                return <Redirect to='/user/dashboard'/>\r\n            }\r\n        }\r\n        if(isAuthenticated()){\r\n            return <Redirect to='/'/>\r\n        }\r\n    }\r\n    const loadingMessage = ()=>{\r\n         return(\r\n             loading && (\r\n                 <div className=\"alert alert-info\">\r\n                     <h2>Loading ....</h2>\r\n                 </div>\r\n             )\r\n         )\r\n    }\r\n        \r\n    const errorMessage = ()=>{\r\n        return (\r\n     <div className=\"row\">\r\n    <div className=\"col-md-6 offset-md-3 text-left\">\r\n        <div className=\"alert alert-danger\" style={{display : error ? \"\" :\"none\"}}>\r\n           {error}\r\n        </div>\r\n        </div> </div>\r\n        \r\n        \r\n    )}\r\n\r\n\r\n\r\n\r\n    const signInForm = () =>{\r\n        return (\r\n            <div className=\"row\">\r\n                <div className=\"col-md-6 offset-md-3 text-left\">\r\n                    <form>\r\n                        <div className=\"form-group\">\r\n                            <label className=\"text-light\">Email</label>\r\n                            <input onChange={handleChange(\"email\")} value={email} className=\"form-control\" type =\"email\"/>\r\n                        </div>\r\n                        <div className=\"form-group\">\r\n                            <label className=\"text-light\">Password</label>\r\n                            <input onChange={handleChange(\"password\")} value={password} className=\"form-control\" type =\"password\"/>\r\n                        </div>\r\n                        <button onClick={onSubmit} className=\"btn btn-success btn-block\">Submit</button>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    \r\n\r\n    return (\r\n        <div>\r\n            <Base title='Sign in Page' description=\"A page for user to Signin\">\r\n              {loadingMessage()}\r\n              {errorMessage()}\r\n              {signInForm()}\r\n              {performRedirect()}\r\n            </Base>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default Signin;\r\n","C:\\Users\\mouli\\Desktop\\mern-lco\\projfrontend\\src\\auth\\helper\\index.js",[],"C:\\Users\\mouli\\Desktop\\mern-lco\\projfrontend\\src\\auth\\helper\\AdminRoutes.js",[],"C:\\Users\\mouli\\Desktop\\mern-lco\\projfrontend\\src\\auth\\helper\\PrivateRoutes.js",[],"C:\\Users\\mouli\\Desktop\\mern-lco\\projfrontend\\src\\user\\UserDashBoard.js",[],"C:\\Users\\mouli\\Desktop\\mern-lco\\pr